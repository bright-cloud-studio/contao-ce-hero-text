{
    "name":"bright-cloud-studio/contao-ce-hero-text",
    "description":"Adds a new Hero Text content element type to Contao.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["content", "element", "contao", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/contao_ce_hero_text/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/contao_ce_hero_text": "system/modules/contao_ce_hero_text"
            }
        }
    },
    "minimum-stability": "dev"
}
